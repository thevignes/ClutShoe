<main class="main">
  <style>
    /* Modern Product Grid Styling */
    .product-grid-4 {
      padding: 20px 0;
    }

    .product-cart-wrap {
      background: rgba(255, 255, 255, 0.9);
      backdrop-filter: blur(10px);
      border-radius: 20px;
      padding: 15px;
      transition: all 0.4s ease;
      border: 1px solid rgba(255, 255, 255, 0.2);
      box-shadow: 0 8px 32px rgba(31, 38, 135, 0.15);
      overflow: hidden;
    }

    .product-cart-wrap:hover {
      transform: translateY(-10px);
      box-shadow: 0 15px 40px rgba(31, 38, 135, 0.2);
    }

    /* Image Container */
    .product-img-action-wrap {
      position: relative;
      overflow: hidden;
      border-radius: 15px;
    }

    .product-img {
      position: relative;
      overflow: hidden;
      border-radius: 15px;
    }

    .product-img img {
      width: 100%;
      height: 300px;
      object-fit: cover;
      transition: transform 0.6s ease;
    }

    .product-img:hover img.default-img {
      transform: scale(0);
      opacity: 0;
    }

    .product-img:hover img.hover-img {
      transform: scale(1);
      opacity: 1;
    }

    .hover-img {
      position: absolute;
      top: 0;
      left: 0;
      opacity: 0;
      transform: scale(1.2);
    }

    /* Product Badges */
    .product-badges {
      position: absolute;
      top: 15px;
      left: 15px;
      z-index: 2;
    }

    .hot {
      background: linear-gradient(45deg, #ff4757, #ff6b81);
      color: white;
      padding: 8px 15px;
      border-radius: 25px;
      font-weight: 600;
      font-size: 12px;
      box-shadow: 0 4px 15px rgba(255, 71, 87, 0.3);
      animation: pulse 2s infinite;
    }

    @keyframes pulse {
      0% {
        transform: scale(1);
      }
      50% {
        transform: scale(1.05);
      }
      100% {
        transform: scale(1);
      }
    }

    /* Product Content */
    .product-content-wrap {
      padding: 15px 10px;
    }

    .product-category {
      margin-bottom: 10px;
    }

    .product-category a {
      color: #006a67 !important;
      font-size: 14px !important;
      font-weight: 500;
      text-transform: uppercase;
      letter-spacing: 1px;
      transition: all 0.3s ease;
    }

    .product-category a:hover {
      color: #00857f !important;
    }

    .product-category h2 {
      font-size: 18px !important;
      margin: 10px 0;
      font-weight: 600;
      line-height: 1.4;
    }

    .product-category h2 a {
      color: #2d3436 !important;
      transition: all 0.3s ease;
    }

    .product-category h2 a:hover {
      color: #006a67 !important;
    }

    /* Price Styling */
    .price-container {
      display: flex;
      align-items: center;
      gap: 10px;
      margin: 15px 0;
    }

    .sale-price {
      color: #006a67 !important;
      font-size: 22px !important;
      font-weight: 700;
    }

    .regular-price {
      color: #a4b0be !important;
      font-size: 16px !important;
      text-decoration: line-through;
    }

    /* Add to Cart Button */
    .product-action-1 {
      position: absolute;
      bottom: -50px;
      left: 0;
      right: 0;
      background: rgba(255, 255, 255, 0.9);
      padding: 15px;
      transition: all 0.3s ease;
      backdrop-filter: blur(5px);
      border-top: 1px solid rgba(255, 255, 255, 0.2);
    }

    .product-cart-wrap:hover .product-action-1 {
      bottom: 0;
    }

    .action-btn {
      background: linear-gradient(45deg, #006a67, #00857f);
      color: white !important;
      width: 45px;
      height: 45px;
      border-radius: 50%;
      display: inline-flex;
      align-items: center;
      justify-content: center;
      transition: all 0.3s ease;
      box-shadow: 0 5px 15px rgba(0, 106, 103, 0.2);
    }

    .action-btn:hover {
      transform: translateY(-3px) rotate(360deg);
      box-shadow: 0 8px 20px rgba(0, 106, 103, 0.3);
    }

    .action-btn i {
      font-size: 20px;
    }

    /* Responsive Design */
    @media (max-width: 768px) {
      .product-img img {
        height: 200px;
      }

      .product-category h2 {
        font-size: 16px !important;
      }

      .sale-price {
        font-size: 18px !important;
      }
    }

    /* Enhanced Glass Effect Styling */
    .product-cart-wrap {
      background: rgba(255, 255, 255, 0.7);
      backdrop-filter: blur(15px) saturate(180%);
      -webkit-backdrop-filter: blur(15px) saturate(180%);
      border: 1px solid rgba(255, 255, 255, 0.3);
      box-shadow: 0 8px 32px rgba(31, 38, 135, 0.15),
        inset 0 0 0 1px rgba(255, 255, 255, 0.4);
      border-radius: 24px;
      padding: 20px;
      transition: all 0.4s cubic-bezier(0.4, 0, 0.2, 1);
      position: relative;
      overflow: hidden;
    }

    .product-cart-wrap::before {
      content: "";
      position: absolute;
      top: 0;
      left: -100%;
      width: 100%;
      height: 100%;
      background: linear-gradient(
        90deg,
        transparent,
        rgba(255, 255, 255, 0.2),
        transparent
      );
      transition: 0.5s;
    }

    .product-cart-wrap:hover {
      transform: translateY(-10px) scale(1.02);
      box-shadow: 0 15px 40px rgba(31, 38, 135, 0.2),
        inset 0 0 0 2px rgba(255, 255, 255, 0.6);
    }

    .product-cart-wrap:hover::before {
      left: 100%;
    }

    /* Enhanced Image Container */
    .product-img-action-wrap {
      background: rgba(255, 255, 255, 0.3);
      backdrop-filter: blur(5px);
      border-radius: 20px;
      padding: 10px;
      border: 1px solid rgba(255, 255, 255, 0.2);
    }

    .product-img {
      border-radius: 16px;
      overflow: hidden;
      box-shadow: 0 5px 15px rgba(0, 0, 0, 0.08);
    }

    /* Enhanced Product Content */
    .product-content-wrap {
      background: rgba(255, 255, 255, 0.5);
      backdrop-filter: blur(10px);
      border-radius: 16px;
      padding: 20px;
      margin-top: 15px;
      border: 1px solid rgba(255, 255, 255, 0.2);
    }

    /* Enhanced Price Container */
    .price-container {
      background: rgba(255, 255, 255, 0.4);
      padding: 10px 15px;
      border-radius: 12px;
      backdrop-filter: blur(5px);
      border: 1px solid rgba(255, 255, 255, 0.2);
    }

    /* Enhanced Action Button */
    .product-action-1 {
      background: rgba(255, 255, 255, 0.8);
      backdrop-filter: blur(10px);
      border-top: 1px solid rgba(255, 255, 255, 0.3);
    }

    .action-btn {
      background: linear-gradient(135deg, #006a67, #00857f);
      backdrop-filter: blur(5px);
      border: 1px solid rgba(255, 255, 255, 0.3);
      box-shadow: 0 8px 20px rgba(0, 106, 103, 0.2),
        inset 0 0 0 1px rgba(255, 255, 255, 0.2);
    }

    /* Enhanced Offer Badge */
    .hot {
      background: linear-gradient(135deg, #ff4757, #ff6b81);
      backdrop-filter: blur(5px);
      border: 1px solid rgba(255, 255, 255, 0.3);
      box-shadow: 0 4px 15px rgba(255, 71, 87, 0.3),
        inset 0 0 0 1px rgba(255, 255, 255, 0.2);
    }

    /* Add a subtle gradient background to the page */
    .main {
      background: linear-gradient(135deg, #f5f7fa, #e3e6e8);
      min-height: 100vh;
    }

    /* Enhanced hover effects */
    .product-category a:hover {
      text-shadow: 0 0 10px rgba(0, 106, 103, 0.3);
    }

    .action-btn:hover {
      transform: translateY(-3px) rotate(360deg);
      box-shadow: 0 12px 25px rgba(0, 106, 103, 0.3),
        inset 0 0 0 2px rgba(255, 255, 255, 0.4);
    }

    /* Shimmer effect on hover */
    @keyframes shimmer {
      0% {
        background-position: -100% 0;
      }
      100% {
        background-position: 100% 0;
      }
    }

    .product-cart-wrap:hover .product-content-wrap {
      background: linear-gradient(
        90deg,
        rgba(255, 255, 255, 0.5),
        rgba(255, 255, 255, 0.8),
        rgba(255, 255, 255, 0.5)
      );
      background-size: 200% 100%;
      animation: shimmer 2s infinite;
    }

    /* Responsive styles for hero slider */
    .hero-slider-1 {
      position: relative;
      overflow: hidden;
    }

    .single-hero-slider {
      padding: 2rem 0;
    }

    .slider-animated-1 {
      display: flex;
      align-items: center;
    }

    .hero-slider-content-2 {
      padding: 0 1rem;
    }

    .hero-slider-content-2 h4 {
      font-size: 1.2rem;
      margin-bottom: 1rem;
      color: #ff6b6b;
    }

    .hero-slider-content-2 h2 {
      font-size: 2.5rem;
      margin-bottom: 1rem;
      line-height: 1.2;
    }

    .hero-slider-content-2 h1 {
      font-size: 3rem;
      margin-bottom: 1rem;
      color: #006A67;
    }

    .hero-slider-content-2 p {
      font-size: 1.1rem;
      margin-bottom: 1.5rem;
    }

    .single-slider-img img {
      max-width: 100%;
      height: auto;
      border-radius: 20px;
      transition: transform 0.3s ease;
    }

    @media (max-width: 992px) {
      .slider-animated-1 {
        flex-direction: column;
      }

      .hero-slider-content-2 {
        text-align: center;
        margin-bottom: 2rem;
        order: 1;
      }
      .single-slider-img {
        order: 2;
        margin-top: 1rem;
      }

      .hero-slider-content-2 h2 {
        font-size: 2rem;
      }

      .hero-slider-content-2 h1 {
        font-size: 2.2rem;
      }

      .hero-slider-content-2 p {
        font-size: 1rem;
        padding: 0 1rem;
      }

      .col-lg-5, .col-lg-7 {
        width: 100%;
        max-width: 100%;
        flex: 0 0 100%;
      }
    }

    @media (max-width: 768px) {
      .single-hero-slider {
        padding: 1.5rem 0;
      }

      .hero-slider-content-2 h4 {
        font-size: 1rem;
      }

      .hero-slider-content-2 h2 {
        font-size: 1.8rem;
      }

      .hero-slider-content-2 h1 {
        font-size: 2rem;
      }

      .hero-slider-content-2 p {
        font-size: 0.9rem;
        padding: 0 0.5rem;
    }

      .single-slider-img img {
        border-radius: 15px;
        max-width: 90%;
        margin: 0 auto;
        display: block;
      }
    }

    @media (max-width: 576px) {
      .single-hero-slider {
        padding: 1rem 0;
      }

      .hero-slider-content-2 h2 {
        font-size: 1.5rem;
    }

    .hero-slider-content-2 {
      padding: 2rem;
    }

      .hero-slider-content-2 p {
        font-size: 0.85rem;
        margin-bottom: 1rem;
      }

      .btn-brush {
        padding: 0.5rem 1.5rem;
        font-size: 0.9rem;
      }
    }
  </style>
